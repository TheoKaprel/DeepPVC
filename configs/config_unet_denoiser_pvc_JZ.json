{
  "dataset_path": ["/gpfsscratch/rech/jxz/uyo34ub/datasets/train_dataset_psf/train_dataset_psf.h5"],
  "test_dataset_path": ["/gpfsscratch/rech/jxz/uyo34ub/datasets/test_dataset_psf/test_dataset_psf.h5"],
  "validation_dataset_path": ["/gpfsscratch/rech/jxz/uyo34ub/datasets/validation_dataset_iec/validation_dataset_iec.h5"],
  "jean_zay": true,
  "compile": true,
  "store_dataset": false,
  "split_dataset": false,
  "num_workers": 10,
  "datatype": "h5",
  "dtype": "float16",
  "with_rec_fp": true,
  "with_att":true,
  "inputs":"full_sino",
  "sino": false,
  "input_eq_angles": 1,
  "with_adj_angles": true,
  "nb_adj_angles": 16,
  "dim": "3d",
  "denoise":true,
  "paths":false,
  
  "max_nb_data": 5000,
  "training_batchsize":1,
  "test_batchsize":1,
  "data_augmentation": ["noise"],
  "data_normalisation": "3d_max",
  "pad":"circular",
  "patches":false,
  "amp": true,
  "verbose": 2,

  "network": "unet_denoiser_pvc",

  "n_epochs":100,
  "learning_rate":0.0002,
  "lr_policy": ["multiplicative",0.5,20],
  "early_stopping":true,
  "layer_norm": "inst_norm",
  "use_dropout": true,
  "leaky_relu": 0.02,
  "ed_blocks": ["conv-norm-relu-conv-norm-relu-pool", "convT-norm-relu-conv-norm-relu"],
  "residual_layer": true,
  "resunet": true,
  "attention": false,
  "save_every_n_epoch": 9999,
  "show_every_n_epoch": 9999,
  "test_every_n_epoch": 1,
  "optimizer":"Adam",
  "device":"cuda",
  "validation_norm": "L1",
  "comment": "",

  "DCNN": false,
  "init_feature_kernel": 3,
  "nb_ed_layers":3,
  "hidden_channels_unet": 16,
  "unet_activation": "relu",
  "recon_loss":["L1", "lesion"],
  "lambda_recon":[1, 100]
}
