{
  "dataset_path": ["/gpfsscratch/rech/jxz/uyo34ub/datasets/train_dataset"],
  "test_dataset_path": ["/gpfsscratch/rech/jxz/uyo34ub/datasets/test_dataset"],
  "jean_zay": true,
  "store_dataset": false,
  "split_dataset": true,
  "num_workers": 16,
  "datatype": "npy",
  "dtype": "float16",
  "merged": true,
  "with_noise": true,
  "with_rec_fp": false,
  "max_nb_data": 600000,
  "training_batchsize":1024,
  "test_batchsize":256,
  "data_augmentation": [],
  "data_normalisation": "img_mean",
  "amp": true,
  "verbose": 2,

  "network": "unet",

  "n_epochs":30,
  "learning_rate":0.0001,
  "lr_policy": ["multiplicative",0.5,20],
  "input_eq_angles":4,
  "with_adj_angles": true,
  "conv3d": false,
  "layer_norm": "inst_norm",
  "use_dropout": true,
  "leaky_relu": 0.02,
  "ed_blocks": "conv-norm-relu-conv-relu-norm",
  "residual_layer": true,
  "resunet":false,
  "attention": false,
  "save_every_n_epoch": 9999,
  "show_every_n_epoch": 9999,
  "test_every_n_epoch": 1,
  "optimizer":"Adam",
  "device":"cuda",
  "validation_norm": "L1",
  "comment": "",

  "init_feature_kernel": 5,
  "nb_ed_layers": 4,
  "hidden_channels_unet":32,
  "unet_activation": "relu",
  "recon_loss":["L1"],
  "lambda_recon":[1]
}
